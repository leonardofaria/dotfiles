{
  "title": "Zoom",
  "rules": [
    {
      "description": "Mute/unmute",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "us.zoom.xos"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": []
            }
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": [
                "left_command",
                "left_shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Start/stop video",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "us.zoom.xos"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "2",
            "modifiers": {
              "mandatory": []
            }
          },
          "to": [
            {
              "key_code": "v",
              "modifiers": [
                "left_command",
                "left_shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Leave",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "us.zoom.xos"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "3",
            "modifiers": {
              "mandatory": []
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}